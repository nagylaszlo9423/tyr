{
  "name": "tyr-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "start:analyze": "vue-cli-service serve --mode analyze",
    "build": "vue-cli-service build",
    "build:analyze": "vue-cli-service build --mode analyze",
    "lint": "vue-cli-service lint",
    "start:pwa": "cross-env PWA=true && npm start"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-brands-svg-icons": "^5.12.1",
    "@fortawesome/free-regular-svg-icons": "^5.12.1",
    "@fortawesome/free-solid-svg-icons": "^5.12.1",
    "@fortawesome/vue-fontawesome": "^0.1.9",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "bootstrap-vue": "^2.4.0",
    "core-js": "^3.6.4",
    "jquery": "^3.4.1",
    "normalize.css": "^8.0.1",
    "ol": "^5.3.3",
    "path": "^0.12.7",
    "popper.js": "^1.16.1",
    "rxjs": "^6.5.4",
    "tyr-api": "git+https://899d2b9e2342eddbe3dc22c4c8771c24c0fdc814:x-oauth-basic@github.com/nagylaszlo9423/tyr-api.git#master",
    "vee-validate": "^3.2.4",
    "vue": "^2.6.11",
    "vue-class-component": "^7.2.2",
    "vue-i18n": "^8.15.3",
    "vue-property-decorator": "^8.4.0",
    "vue-router": "^3.1.5",
    "vue-toasted": "^1.1.27",
    "vue-touch": "^2.0.0-beta.4",
    "vue2-simplert-plugin": "^0.5.3",
    "vuex": "^3.1.2",
    "vuex-persistedstate": "^2.7.1"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/jquery": "^3.3.32",
    "@types/ol": "^5.3.7",
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-plugin-typescript": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "@vue/eslint-config-typescript": "^4.0.0",
    "babel-eslint": "^10.0.1",
    "cross-env": "^6.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2",
    "lint-staged": "^10.0.7",
    "node-sass": "^4.13.1",
    "sass-loader": "^8.0.2",
    "typescript": "^3.5.3",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.41.5",
    "webpack-bundle-analyzer": "^3.6.0",
    "webpack-cli": "^3.3.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended",
      "@vue/typescript"
    ],
    "rules": {
      "no-extra-semi": "error",
      "semi": "error",
      "semi-spacing": "error",
      "semi-style": "error",
      "quotes": [
        "error",
        "single"
      ],
      "no-console": "warn",
      "vue/require-v-for-key": "off"
    },
    "parserOptions": {
      "parser": "@typescript-eslint/parser",
      "ecmaFeatures": {
        "legacyDecorators": true
      }
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "vue-cli-service lint",
      "git add"
    ],
    "*.vue": [
      "vue-cli-service lint",
      "git add"
    ]
  }
}
